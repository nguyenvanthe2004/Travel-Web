{
  "compilerOptions": {
    "ignoreDeprecations": "6.0",

    "target": "ES2020",
    "lib": ["DOM", "ES2020"],
    "jsx": "react-jsx",

    "module": "ESNext",
    "moduleResolution": "Bundler",

    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "types": ["vite/client"]
  },
  "include": ["src"]
}
